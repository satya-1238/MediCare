<link rel="stylesheet" href="/css/signIn.css">
<div class="outer">
      <div class="inner">
        <h2>Sign In ~ Doctors</h2>
        <form action="/doctors/createSession" method="post">
            <input type="email" name="email" id="" placeholder="Email" required>
            <input type="password" name="password" id="" placeholder="Password" required>
            <input type="submit" value="SignIn">
        </form>
        <p>
          <a href="/doctors/sign-up">Not have an Account? SignUp!</a>
        </p>
        <p>
          <a href="../doctors/auth/google">SignIn/SignUp with google</a>
        </p>
      </div>
      <div class="inner">
        <h2>Sign In ~ Patients</h2>
        <form action="/patients/createSession" method="post">
            <input type="email" name="email" id="" placeholder="Email" required>
            <input type="password" name="password" id="" placeholder="Password" required>

            <input type="submit" value="SignIn">
        </form>
        <p>
          <a href="/patients/sign-up">Not have an Account? SignUp!</a>
        </p>
        <p>
          <a href="../patients/auth/google">SignIn/SignUp with google</a>
        </p>
      </div>
</div>